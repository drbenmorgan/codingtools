#cmakedefine HAS_CXX11_TYPE_TRAITS
#cmakedefine HAS_CXX11_UTILITY_DECLVAL
#cmakedefine HAS_CXX11_VARIADIC_TEMPLATES

#endif // EXAMPLE_HEADER.HPP_HH

